{
  "hosting": {
    "public": "dist",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "site": "schmelczer-dev",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "redirects": [
      {
        "source": "/fizika",
        "destination": "https://fizika.schmelczer.dev",
        "type": 301
      }
    ]
  }
}
